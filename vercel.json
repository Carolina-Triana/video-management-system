{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/videos(.*)",
      "dest": "/api/videos.ts"
    },
    {
      "src": "/api",
      "dest": "/api/index.ts"
    }
  ],
  "installCommand": "cd backend && npm install",
  "buildCommand": "cd backend && npm run build"
}
